<!-- Modal -->
<div class="tab-pane fade active show" id="edit-item" role="tabpanel" aria-labelledby="profile-tab">
    <div class="card shadow mt-3">
        <div class="card-body">
            <form action="/admin/delivery/schedule/<%= schedule.id %>?_method=PUT" method="POST">
                    <div class="form-group">
                        <label for="sm">SM</label>
                        <input type="text" class="form-control" name="sm" value="<%= schedule.sm %>"
                            required>
                    </div>
                    <div class="form-group">
                        <label for="name">Nama Mould</label>
                        <input type="text" class="form-control" name="name"
                            value="<%= schedule.name %>"
                            required>
                    </div>

                     <div class="form-group">
                         <label for="name">Driver</label>
                         <select name="driver" class="form-control">
                             <option value="<%= schedule.driver %>"><%= schedule.driver %></option>
                             <option value="Nama Driver">Nama Driver</option>
                             <option value="Nama Driver">Nama Driver</option>
                         </select>
                     </div>

                     <div class="form-group">
                         <label for="kendaraan">Kendaraan</label>
                         <select name="kendaraan" class="form-control">
                             <option value="<%= schedule.kendaraan %>"><%= schedule.kendaraan %></option>
                             <option value="Grand Max">Grand Max</option>
                             <option value="Grand Max">Grand Max</option>
                         </select>
                     </div>

                    <div class="form-group">
                        <label for="name">Keterangan</label>
                        <select name="keterangan" class="form-control">
                            <option value="<%= schedule.keterangan %>"><%= schedule.keterangan %></option>
                            <option value="Kirim">Kirim</option>
                            <option value="Ambil">Ambil</option>
                        </select>
                    </div>

                    <div class="form-group">
                        <label for="name">Status</label>
                        <select name="status" class="form-control">
                            <option value="<%= schedule.status %>"><%= schedule.status %></option>
                            <option value="Tunggu">Tunggu</option>
                            <option value="On The Way">On The Way</option>
                            <option value="Finish">Finish</option>
                        </select>
                    </div>
                    <div class="form-group">
                        <label for="name">Tujuan</label>
                        <input type="text" class="form-control" id="name" name="tujuan" value="<%= schedule.tujuan %>"
                            required>
                    </div>
                <div class="modal-footer">
                    <input type="hidden" value="<%= schedule.id %>" name="id">
                    <input type="hidden" value="<%= schedule.deliveryId %>" name="deliveryId">
                    <button type="submit" class="btn btn-primary">Add</button>
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                </div>
            </form>
        </div>
    </div>
</div>

 
 