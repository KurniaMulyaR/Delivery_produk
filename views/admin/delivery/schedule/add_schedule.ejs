<!-- Modal -->
<div class="modal fade" id="add-modal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
    aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Add Tanggal Pengiriman</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <form action="/admin/delivery/add/schedule" method="POST">
                <div class="modal-body">
                <div class="form-group">
                    <label for="sm">SM</label>
                    <input type="text" class="form-control" id="sm" name="sm" placeholder="Enter SM" required>
                </div>
                <div class="form-group">
                    <label for="name">Nama Mould</label>
                    <input type="text" class="form-control" id="name" name="name" placeholder="Enter Name Mould"
                        required>
                </div>

                <div class="form-group">
                    <label for="name">Driver</label>
                    <select name="driver" class="form-control">
                        <option value="-">---</option>
                        <option value="Nama Driver">Nama Driver</option>
                        <option value="Nama Driver">Nama Driver</option>
                    </select>
                </div>

                <div class="form-group">
                    <label for="kendaraan">Kendaraan</label>
                    <select name="kendaraan" class="form-control">
                        <option value="-">---</option>
                        <option value="Grand Max">Grand Max</option>
                        <option value="Grand Max">Grand Max</option>
                    </select>
                </div>

                <div class="form-group">
                    <label for="name">Keterangan</label>
                    <select name="keterangan" class="form-control">
                        <option value="">-- Keterangan --</option>
                        <option value="Kirim">Kirim</option>
                        <option value="Ambil">Ambil</option>
                    </select>
                </div>

                <div class="form-group">
                    <label for="name">Status</label>
                    <select name="status" class="form-control">
                        <option value="-">---</option>
                        <option value="Tunggu">Tunggu</option>
                        <option value="On The Way">On The Way</option>
                        <option value="Finish">Finish</option>
                    </select>
                </div>

                <div class="form-group">
                    <label for="name">Tujuan</label>
                    <input type="text" class="form-control" id="name" name="tujuan" placeholder="Enter Tujuan" required>
                </div>

                <input type="hidden" name="deliveryId" value="<%= deliveryId %>">
                <button type="submit" class="btn btn-primary">Save</button>
            </div>
            </form>
        </div>
    </div>
</div>